@using ServiceRecordFrontend.Models;

<tr class="record-item">
    <td>@record.VIN</td>
    <td>@record.Make</td>
    <td>@record.Model</td>
    <td>@record.Year</td>
    <td>@record.Owner</td>
    <td>@record.License</td>
    <td>@record.Mileage</td>
    <td class="service-cell">@record.Service</td>
    <td>@record.Charge</td>
    <td>@record.Date</td>
</tr>

@code {
    [Parameter]
    public Record? record { get; set; }
}